<?xml version="1.0"?>
<extension key="coop.palantetech.module.cpehn.joinnetworkcheckbox" type="module">
  <file>joinnetworkcheckbox</file>
  <name>CPEHN Join Network checkbox</name>
  <description>Adds a checkbox to Contribution/Event Pages that allows someone to join the CPEHN Network, but not to unjoin.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jon Goldberg</author>
    <email>jon@palantetech.coop</email>
  </maintainer>
  <releaseDate>2014-05-15</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.2</ver>
    <ver>4.3</ver>
    <ver>4.4</ver>
    <ver>4.5</ver>
    <ver>4.6</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Joinnetworkcheckbox</namespace>
  </civix>
</extension>
